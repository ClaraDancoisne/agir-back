// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../../../../node_modules/@prisma/client_stats"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_STATS_URL")
}

model TestTable {
  id   String @id
  type String
}
